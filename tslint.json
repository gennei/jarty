{
    "rules": {
        "bitwise": false,
        "classname": true,
        "curly": true,
        "debug": true,
        "dupkey": true,
        "eofline": true,
        "eqeqeq": [true, "allow-null-check"],
        "evil": false,
        "forin": true,
        "indent": [true, 4],
        "labelpos": true,
        "label-undefined": true,
        "maxlen": [true, 140],
        "noarg": true,
        "noconsole": [true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "noconstruct": true,
        "nounreachable": true,
        "noempty": false,
        "oneline": [true,
            "check-open-brace",
            "check-catch",
            "check-else",
            "check-whitespace"
        ],
        "quotemark": false,
        "radix": true,
        "semicolon": true,
        "sub": false,
        "trailing": true,
        "varname": false,
        "whitespace": [true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator"
        ]
    }
}
